{
  "extends": [
    "config:base"
  ],
  "packageRules": [
    {
      "matchPackageNames": [
        "com.github.GeyserMC:MCProtocolLib",
        "com.github.GeyserMC:mcauthlib",
        "org.cloudburstmc.netty:netty-transport-raknet",
        "eclipse-temurin"
      ],
      "enabled": false
    },
    {
      "matchUpdateTypes": [
        "minor",
        "patch",
        "pin",
        "digest"
      ],
      "automerge": true
    }
  ]
}
