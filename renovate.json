{
  "extends": ["config:base", "group:allNonMajor", ":semanticCommits"],
  "ignoreDeps": [
    "net.fabricmc:intermediary",
    "eclipse-temurin"
  ],
  "packageRules": [
    {
      "matchUpdateTypes": [
        "minor",
        "patch",
        "pin",
        "digest"
      ],
      "automerge": true,
      "automergeType": "branch"
    },
    {
      "matchPackageNames": ["**"],
      "allowedVersions": "!/\\-SNAPSHOT$/"
    }
  ]
}
